# Uncomment this line to define a global platform for your project
platform :ios, '8.0'
# Uncomment this line if you're using Swift
use_frameworks!

def shared_pods
  pod 'GoogleMaps'
end

target 'Scout' do
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'AlamofireImage'
  pod 'MBProgressHUD'
  pod 'DateTools'
  pod 'FontAwesome.swift'
  pod 'BuddyBuildSDK'
  pod 'Parse'
  pod 'ParseFacebookUtilsV4'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  shared_pods
end

target 'ScoutTests' do
  shared_pods
end

